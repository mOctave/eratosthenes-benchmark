{
    "iris":{
        "id":"iris",
        "language":"Iris",
        "prereq":"iris",
        "buildcmd":"iris build Iris.iris",
        "runcmd":"./iris"
    },
    "nodejs":{
        "id":"nodejs",
        "language":"NodeJS",
        "prereq":"node -v",
        "runcmd":"node NodeJS.js"
    },
    "golang":{
        "id":"golang",
        "language":"Go",
        "prereq":"go version",
        "buildcmd":"go build GoLang.go",
        "runcmd":"./GoLang"
    },
    "python":{
        "id":"python",
        "language":"Python",
        "prereq":"python3 --version",
        "runcmd":"python3 Python.py"
    },
    "lua":{
        "id":"lua",
        "language":"Lua",
        "prereq":"lua -v",
        "runcmd":"lua Lua.lua"
    },
    "java":{
        "id":"java",
        "language":"Java",
        "prereq":"java --version",
        "buildcmd":"javac Java.java",
        "runcmd":"java Java"
    },
    "cpp":{
        "id":"cpp",
        "language":"C++",
        "prereq":"clang++ --version",
        "buildcmd":"clang++ CPP.cpp -std=c++11",
        "runcmd":"./a.out"
    },
    "ruby":{
        "id":"ruby",
        "language":"Ruby",
        "prereq":"ruby -v",
        "runcmd":"ruby Ruby.rb"
    },
    "perl":{
        "id":"perl",
        "language":"Perl",
        "prereq":"perl -v",
        "runcmd":"perl Perl.pl"
    },
    "swift":{
        "id":"swift",
        "language":"Swift",
        "prereq":"swift --help",
        "buildcmd":"swiftc Swift.swift -parse-as-library",
        "runcmd":"./Swift"
    },
    "julia":{
        "id":"julia",
        "language":"Julia",
        "prereq":"julia -v",
        "runcmd":"julia Julia.jl"
    },
    "fortran":{
        "id":"fortran",
        "language":"Fortran",
        "prereq":"gfortran --version",
        "buildcmd":"gfortran Fortran.f03",
        "runcmd":"./a.out"
    },
    "php":{
        "id":"php",
        "language":"PHP",
        "prereq":"php -v",
        "runcmd":"php PHP.php"
    },
    "csharp":{
        "id":"csharp",
        "language":"C#",
        "prereq":"mono -V",
        "buildcmd":"csc CSharp.cs",
        "runcmd":"mono CSharp.exe"
    }
}
